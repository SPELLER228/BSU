#include <iostream>

using namespace std;

int main()
{
	int a, k, raz=1, c, sum1=0, sum2=0;
	cin >> a >> k;
	c = a;
	cout << "sum1 = ";
	for (int i=0;i<k || c<=0;i++)
	{ 
		sum1 = sum1+c%10;
	  c = c / 10;
	  cout << sum1<<' ';
	}
	c = a;
	cout << endl;
	while (c > 9)
	{
			c =c / 10;
			raz=raz*10;
			cout << raz << ' ';
	}
	c = a;
	cout << endl << "sum2 = ";
	for (int i=0;i < k;i++)
	{
		sum2 = sum2 + c / raz % 10;
		raz = raz / 10;
		cout << sum2 << ' ';
	}
	cout << endl;
	cout << sum1 << ' ' << sum2 << endl;
	if (sum1 == sum2)
		cout << "ravnie";
		else cout << "ne ravnie";
	return 0;
}
